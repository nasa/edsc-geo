{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "@edsc"
  ],
  "overrides": [
    {
      "files": [
        "*.test.js"
      ],
      "rules": {
        "import/first": 0
      }
    },
    {
      "files": [
        "serverless/src/**/*"
      ],
      "rules": {
        "import/prefer-default-export": 0
      }
    }
  ]
}
